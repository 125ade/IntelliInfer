{
  "openapi": "3.0.0",
  "info": {
    "title": "IntelliInfer",
    "version": "1.0.0",
    "description": "A simple Express API application with Swagger documentation to implement inference in computer vision model"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/check/health": {
      "get": {
        "summary": "Health Check",
        "description": "Returns the health status of the system",
        "responses": {
          "200": {
            "description": "System is online",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "system": {
                      "type": "string",
                      "example": "online"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/model/list": {
    "get": {
      "summary": "List of AI models",
      "description": "Returns the list of the available models in the system",
      "responses": {
        "200": {
          "description": "List of the models",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "integer",
                      "example": 1
                    },
                    "name": {
                      "type": "string",
                      "example": "Nome esempio"
                    },
                    "description": {
                      "type": "string",
                      "example": "Descrizione esempio"
                    },
                    "pathweights": {
                      "type": "string",
                      "example": "Percorso pesi esempio"
                    },
                    "architecture": {
                      "type": "string",
                      "example": "yolo"
                    },
                    "created_at": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-06-02T13:11:03.958Z"
                    },
                    "updated_at": {
                      "type": "string",
                      "format": "date-time",
                      "example": "2024-06-02T13:11:03.958Z"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  }
}
